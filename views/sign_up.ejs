  <body class="text-center">
    <form action="/sign_up" class="form-signin" method="POST">
  <img class="mb-4" src="/css/logo.png" alt="" width="72" height="72">
  <h1 class="h3 mb-3 font-weight-normal">Sign Up</h1>
  <% if(typeof errors != 'undefined') { %>
    <% errors.forEach(function(error){ %>
      <div class="alert alert-info" role="alert">
        <%= error.msg %>
      </div>
    <% }) %>
  <% } %>   
 
  <input type="text"  class="form-control" name ="name" placeholder="Name" required autofocus>
  <input type="email"  class="form-control" name = "email" placeholder="Your E-mail" required>
  <input type="password" class="form-control" name = "pass" placeholder="Set Password" required>
  <input type="password" class="form-control" name = "pass2" placeholder="Confirm your Password" required>

  <button class="btn btn-lg btn-primary btn-block" type="submit">Create Account</button>
  <p>Already have an account? </p>
  <h4 style="margin:0;padding:0;"><a href="/sign_in">Sign in</a></h4>
  <a href="/auth/google">click here</a>
</form>
